<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Super Track Urgent Care: Urgent Care: Bellingham, WA</title>
    <link rel="icon" href="https://sa1s3optim.patientpop.com/assets/docs/42922.png" type="image/x-icon">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>
        #main {
            padding-top: 107px;
        }

        @@media (min-width:769px) {
            #main {
                padding-top: 125px !important;
            }
        }
    </style>
</head>
<body>
    <section class="position-fixed w-100" style="z-index:9999999;">
        <header>
            <!-- header -->
            <nav class="navbar navbar-light bg-light">
                <div class="container">
                    <a class="navbar-brand d-none d-lg-block" href="#"><img src="~/Images/SuperTrack-logo.png" class="img-fluid" alt="Responsive image"></a>
                    <ul class="nav float-lg-right">
                        <li class="nav-item pr-1">
                            <a class="nav-link btn btn-secondary border-0 text-uppercase font-weight-sm" href="@Url.Action("Index","Contact")">call us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link btn btn-success border-0 text-uppercase font-weight-sm" href="https://patientportal.advancedmd.com/137838/onlinescheduling/existing" target="_blank">book online</a>
                        </li>
                    </ul>
                </div>
            </nav>
            <!-- /header -->
        </header>
        <!--navigation bar-->
        <nav class="navbar navbar-expand-lg navbar-light bg-white p-lg-0 px-0 pt-0 pb-0" id="navigation-bar">
            <a class="navbar-brand d-block d-lg-none ml-3" href="#"><img src="~/Images/SuperTrack-logo.png" class="img-fluid" alt="Responsive image"></a>
            <button class="navbar-toggler mr-3" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse bg-success" id="navbarSupportedContent">
                <div class="container">
                    <ul class="navbar-nav justify-content-end">
                        <li class="nav-item active py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","Home")">Home</a>
                        </li>
                        <li class="nav-item dropdown py-lg-2 hide-dropdown">
                            <a class="nav-link dropdown-toggle text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","About")" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                About
                            </a>
                            <div class="dropdown-menu bg-secondary m-0 show-dropdown border-0 pb-0" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Provider")">Providers</a>
                            </div>
                        </li>
                        <li class="nav-item dropdown py-lg-2 hide-dropdown">
                            <a class="nav-link dropdown-toggle text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","Services")" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Services
                            </a>
                            <div class="dropdown-menu bg-secondary m-0 show-dropdown border-0 pb-0" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Abscesses and Skin Infections</a>
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Allergies and Asthma</a>
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Bronchitis and Pneumonia</a>
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Cuts and Lacerations</a>
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Sprains and Broken Bones</a>
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Urinary Tract Infection</a>
                                <a class="dropdown-item text-white text-uppercase" href="@Url.Action("Index","Services")">Work Related Injuries</a>
                            </div>
                        </li>
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","Services")">Telemedicine</a>
                        </li>
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","OurClinic")">Our Clinic</a>
                        </li>
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","Insurance")">Insurance</a>
                        </li>
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","PlanYourVisit")">Plan Your Visit</a>
                        </li>
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","Testimonials")">Testimonials</a>
                        </li>                       
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" target="_blank" href="https://patientportal.advancedmd.com/137838/onlinescheduling/existing">Patient Portal</a>
                        </li>
                        <li class="nav-item py-lg-2">
                            <a class="nav-link text-secondary font-weight-sm text-uppercase pl-2 pl-lg-auto" href="@Url.Action("Index","Contact")">Contact</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <!--/navigation bar-->
    </section>
    <div class="body-content" id="main">
        @RenderBody()       
        <footer class="py-5">
                <div class="container py-2">
                    <div class="row justify-content-center">
                        <div class="col-lg-4 text-center mt-2 mt-lg-0">
                            <div class="text-white"> © Copyright @DateTime.Now.Year Supertrack Urgent Care</div>
                        </div>
                        <div class="col-lg-2 text-center mt-2 mt-lg-0">
                            <a href="@Url.Action("Index", "PrivacyPolicy")" class="text-white"> Privacy Policy</a>
                        </div>
                        <div class="col-lg-2 text-center mt-2 mt-lg-0">
                            <a href="@Url.Action("Index", "TermsConditions")" class="text-white">Terms & Conditions</a> 
                        </div>
                        <div class="col-lg-2 text-center mt-2 mt-lg-0">
                            <a href="@Url.Action("Index","Contact")" class="text-white">Contact Us</a>
                        </div>
                    </div>
                    <div class="text-center text-muted mt-5">Super Track Urgent Care, Bellingham, WA</div>
                    <div class="text-center text-muted mt-3">Phone (appointments): 360-746-6531 | Phone (general inquiries): 360-746-6531</div>
                    <div class="text-center text-muted mt-3">Address: 1050 Lakeway Drive, Bellingham, WA 98229</div>
                    <div class="text-center text-muted mt-5">Super Track Urgent Care, Bellingham, WA</div>
                    <div class="text-center text-muted mt-3">Phone (appointments): 360-746-3333 | Phone (general inquiries): (360) 746-3333</div>
                    <div class="text-center text-muted mt-3">Address: 302 36th Street, Bellingham, WA 98225</div>
                    <div class="text-center text-muted mt-5">Super Track Urgent Care, Point Roberts, WA</div>
                    <div class="text-center text-muted mt-3">Phone (appointments): 360-945-2580 | Phone (general inquiries): (360) 945-2580</div>
                    <div class="text-center text-muted mt-3">Address: 2330 Point Roberts, Point Roberts, WA 98281</div>
                    <div class="mt-5 row justify-content-center">
                        <div class="col-lg-2 col-md-4 col-6 text-center">
                            <div class="h4 font-weight-sm text-white">4.61 <sup class="font-weight-lighter">/ 5</sup></div>
                        </div>
                        <div class="col-lg-2 col-md-4 col-6 text-center" style="border-left:1px solid #6c757d;">
                            <span><i class="fas fa-star text-warning"></i></span>  <span><i class="fas fa-star text-warning"></i></span>  <span><i class="fas fa-star text-warning"></i></span>  <span><i class="fas fa-star text-warning"></i></span>  <span><i class="fas fa-star-half-alt text-warning"></i></span>
                            <div class="small text-muted">(23 reviews)</div>
                        </div>
                    </div>              
                </div>

            </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
